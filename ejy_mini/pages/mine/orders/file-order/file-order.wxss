/* pages/mine/orders/file-order/file-order.wxss */
Page{
  background-color: #f5f5f5;
}
.container{
  width: 100%;
  height: 100%;
  display: flex;
  margin-top: 20rpx;
  flex-direction: column;
  align-items: center;

}
.file-order-item{
  color: #333;
  width: 90%;
  background-color: #fff;
  border-radius: 30rpx;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.item{
  width: 100%;
  line-height: 80rpx;
  border-bottom: 1rpx solid #eee;
  height: 80rpx;
  display: flex;
justify-content: space-between;
}
.item-left{
  font-size: 26rpx;
  color: #9b9b9b;
}
.item-right{
  color: #333;
  font-size: 32rpx;
}