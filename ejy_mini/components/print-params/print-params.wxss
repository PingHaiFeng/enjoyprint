  justify-content: center;
  margin-left: auto;
  margin-right: 15rpx;
  align-items: center;
  /* background-color: #5f98fd; */
  color: #333333;
  border-radius: 5px;
  /* position: absolute;
  right: 0px; */
  border: 1rpx #bfbfbf solid;
