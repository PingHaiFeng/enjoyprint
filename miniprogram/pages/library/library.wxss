Page {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  padding: 20rpx;
  align-items: center;
}


SearchBar{
  width: 100%;
}

.folder-item {
  width: 700rpx;
height: 160rpx;
  background-color: #fff;
  border-radius: 20rpx;
  box-sizing: border-box;
  padding: 20rpx;
  display: flex;
  margin: 30rpx 0;


}

.icon-folder {
  width: 70rpx;
  height: 80rpx;
}

.folder-text-wrp {
  width:550rpx;
  display: flex;
  flex-direction: column;
  height: 100%;
  justify-content: space-between;
  margin-left: 25rpx;
}
.folder-top-bar{
  display: flex;
  width: 100%;
  align-items: center;
}
.icon-hot {
  width: 35rpx;
  height: 35rpx;
  margin-left: 20rpx;
}
.down_share{
  
  margin-left: auto;

}

.down_share_img{
  width:35rpx;
  height: 35rpx;
  margin: 0 20rpx;
}
.create-date{
  margin:6rpx 0;
}
.folder-bottom-bar {
  width: 500rpx;
display: flex;
align-items: center;
}

.print-num {
  margin-left: 30rpx;
}