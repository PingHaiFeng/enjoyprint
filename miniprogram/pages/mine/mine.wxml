<view class="mine">
    <view class="up_board">
        <view class="login-wrp" bindtap="getUserProfile">
            <image class="userinfo-avatar" src="{{hasUserInfo?userInfo.avatarUrl:'../../images/userAvatar.png'}}"
                mode="cover"></image>
            <text>{{hasUserInfo?userInfo.nickName:'请登录>'}} </text>
        </view>
    </view>
    <view class="buttom_board">

        <view class="nav" wx:for="{{navList}}" wx:key="id" wx:for-item="item">
            <action-sheet class="action-sheet" navText="{{item.title}}" navIco="{{item.ico}}" navUrl="{{item.navUrl}}"></action-sheet>
        </view>

    </view>


</view>
