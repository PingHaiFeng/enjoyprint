Page{
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
.price-wrp{
  display: flex;
  flex-direction: column;
  width: 90%;
  box-sizing: border-box;
  padding: 0 30rpx;
  margin: 45rpx auto;
  background-color: #fff;
  border-radius: 30rpx;
}
.store-name{
  color: #3d3d3d;
  margin: 35rpx auto;
  font-size: 30rpx;
}
.price-item{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  height:90rpx;
  color: #3d3d3d;
  line-height: 90rpx;
  font-size: 26rpx;
  border-top: 1rpx solid #ebebeb;
}

.none-price{
  width: 100%;
  display: flex;
  margin-top: 400rpx;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  
  }
  .none-price image{
    width:200rpx;
  height: 200rpx;
  }
  .none-price text{
    font-size: 30rpx;
    color: #8a8a8a;
    margin-top: 30rpx;
  }